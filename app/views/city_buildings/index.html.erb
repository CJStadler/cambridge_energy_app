<p id="notice"><%= notice %></p>

<h1>Listing City Buildings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @city_buildings.each do |city_building| %>
      <tr>
        <td><%= link_to 'Show', city_building %></td>
        <td><%= link_to 'Edit', edit_city_building_path(city_building) %></td>
        <td><%= link_to 'Destroy', city_building, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City building', new_city_building_path %>
